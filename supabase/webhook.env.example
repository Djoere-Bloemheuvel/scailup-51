# Webhook Configuration for contact-conversion-webhook Edge Function
# Copy this file to .env and update with your actual values

# N8N Webhook URL - where to send contact conversion notifications
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/contact-conversion

# API Key for authenticating with your webhook endpoint
N8N_API_KEY=your_api_key_here

# Enable/disable webhook sending (set to 'false' to disable)
WEBHOOK_ENABLED=true

# Supabase configuration (already set in most cases)
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key 